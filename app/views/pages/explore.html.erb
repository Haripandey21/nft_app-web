<h1 class="text-center mt-2">These are the listed NFTs</h1>
<div class="container mt-5">
  <table class="table table-bordered border-primary">
    <thead>
      <tr class="text-center">
        <th scope="col">TokenID</th>
        <th scope="col">Name</th>
        <th scope="col">Description</th>
        <th scope="col">Image</th>
      </tr>
    </thead>
    <tbody>
      <% @tables.each do |table|%>
        <tr class="align-middle text-center">
          <td><%= table.token_ID %></td>
          <td><%= table.name %></td>
          <td class="w-25"><%= table.description %></td>
          <td class="w-25 text-center"><img src="<%= table.image_URI %>" class="img-thumbnail w-50" alt=""></td>
        </tr>
      <% end%>
    </tbody>
  </table>
</div>

